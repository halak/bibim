		<Input xsi:type="PhotoshopDocumentToUILayout1">
			<Input xsi:type="ImportPhotoshopDocument" IgnoreImageResources="false" IgnoreLayers="false" IgnoreMergedBitmap="true">
				<Input xsi:type="FileToStream" Input1="$(AssetName).psd" />
			</Input>
		</Input>
		<BitmapSheetSize>
			<Width>1024</Width>
			<Height>1024</Height>
		</BitmapSheetSize>
		<BitmapSheetClusterSize>
			<Width>16</Width>
			<Height>16</Height>
		</BitmapSheetClusterSize>
	</Cook>
</GameAssetRecipe>
